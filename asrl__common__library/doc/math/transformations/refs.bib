	File = {Smith8600.pdf:Smith8600.pdf:PDF},
	Journal = {The International Journal of Robotics Research},
	Number = {4},
	Pages = {56--68},
	Title = {{On the Representation and Estimation of Spatial Uncertainty}},
	Url = {http://ijr.sagepub.com/cgi/content/abstract/5/4/56},
	Volume = {5},
	Year = {1986},
