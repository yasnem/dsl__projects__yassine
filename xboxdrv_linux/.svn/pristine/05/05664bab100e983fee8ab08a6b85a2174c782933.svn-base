  unsigned int thumb_l     :1;
  unsigned int thumb_r     :1;

  // data[3] ------------------
  unsigned int lb          :1;
  unsigned int rb          :1;
  unsigned int guide       :1;
  unsigned int dummy1      :1; // always 0

  unsigned int a           :1; // green
  unsigned int b           :1; // red
  unsigned int x           :1; // blue
  unsigned int y           :1; // yellow

  // data[4] ------------------
